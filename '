$(document).ready(function(){

	$('#emailForm').on('submit', function(e){
		e.preventDefault()
		// get the fields
		var name = $('#name').val()
		var email = $('#email').val()
		var message = $('#message').val()
		// ajax to formspree
		$.ajax({
			url: 'https://formspree.io/info@davidbasil.net'
		})
	})

})
